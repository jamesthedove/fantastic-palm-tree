{
  "name": "typescript-graphql",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "node dist/server",
    "test": "mocha test/*.test.js",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "ts-node --transpile-only src/schema"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "apollo-server": "3.4.0",
    "async": "^3.2.1",
    "graphql": "15.6.1",
    "graphql-scalars": "1.12.0",
    "nexus": "1.1.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.8",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.17.29",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "supertest": "^6.1.6",
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.4.4"
  }
}
